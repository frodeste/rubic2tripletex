{
	"mcpServers": {
		"figma": {
			"type": "http",
			"url": "https://mcp.figma.com/mcp"
		},
		"context7": {
			"command": "npx",
			"args": ["-y", "@upstash/context7-mcp"]
		},
		"github": {
			"command": "npx",
			"args": ["-y", "@modelcontextprotocol/server-github"],
			"env": {
				"GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
			}
		},
		"sentry": {
			"url": "https://mcp.sentry.dev/mcp"
		}
	}
}
