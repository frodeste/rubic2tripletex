# Database (Neon Postgres - provided by Vercel integration)
DATABASE_URL=postgresql://user:password@host/dbname?sslmode=require
DATABASE_URL_UNPOOLED=postgresql://user:password@host/dbname?sslmode=require

# Rubic API
RUBIC_API_BASE_URL=https://rubicexternalapitest.azurewebsites.net
RUBIC_API_KEY=your-rubic-api-key
RUBIC_ORGANIZATION_ID=12345

# Tripletex Production
TRIPLETEX_PROD_ENABLED=false
TRIPLETEX_PROD_BASE_URL=https://tripletex.no/v2
TRIPLETEX_PROD_CONSUMER_TOKEN=your-prod-consumer-token
TRIPLETEX_PROD_EMPLOYEE_TOKEN=your-prod-employee-token

# Tripletex Sandbox
TRIPLETEX_SANDBOX_ENABLED=false
TRIPLETEX_SANDBOX_BASE_URL=https://api.tripletex.io/v2
TRIPLETEX_SANDBOX_CONSUMER_TOKEN=your-sandbox-consumer-token
TRIPLETEX_SANDBOX_EMPLOYEE_TOKEN=your-sandbox-employee-token

# Legacy Tripletex env vars (optional fallback for production)
# TRIPLETEX_API_BASE_URL=https://tripletex.no/v2
# TRIPLETEX_CONSUMER_TOKEN=your-consumer-token
# TRIPLETEX_EMPLOYEE_TOKEN=your-employee-token

# Auth0 (for dashboard authentication) - @auth0/nextjs-auth0 v4
AUTH0_SECRET=use-openssl-rand-hex-32-to-generate
AUTH0_DOMAIN=your-tenant.auth0.com
AUTH0_CLIENT_ID=your-auth0-client-id
AUTH0_CLIENT_SECRET=your-auth0-client-secret
APP_BASE_URL=https://integration.uniteperformance.no

# Vercel Cron (auto-set by Vercel, or set manually for local dev)
CRON_SECRET=your-cron-secret

# Sentry (error tracking and performance monitoring)
NEXT_PUBLIC_SENTRY_DSN=https://your-dsn@o0.ingest.sentry.io/0
SENTRY_DSN=https://your-dsn@o0.ingest.sentry.io/0
SENTRY_ORG=your-sentry-org
SENTRY_PROJECT=your-sentry-project
SENTRY_AUTH_TOKEN=your-sentry-auth-token

# AI Coding Assistants (optional - forwarded into devcontainer via remoteEnv)
# Claude Code: https://console.anthropic.com/settings/keys
ANTHROPIC_API_KEY=
# OpenAI Codex: https://platform.openai.com/api-keys
OPENAI_API_KEY=
