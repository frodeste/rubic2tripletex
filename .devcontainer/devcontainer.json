{
	"name": "Rubic2Tripletex",
	"dockerComposeFile": "docker-compose.yml",
	"service": "app",
	"workspaceFolder": "/workspaces/rubic2tripletex",
	"features": {},
	"customizations": {
		"vscode": {
			"extensions": ["biomejs.biome", "bradlc.vscode-tailwindcss"],
			"settings": {
				"editor.defaultFormatter": "biomejs.biome",
				"editor.formatOnSave": true,
				"editor.codeActionsOnSave": {
					"quickfix.biome": "explicit",
					"source.organizeImports.biome": "explicit"
				}
			}
		}
	},
	"forwardPorts": [3000, 5432],
	"postCreateCommand": "bun install",
	"remoteEnv": {
		"DATABASE_URL": "postgresql://postgres:postgres@localhost:5432/rubic2tripletex",
		"DATABASE_URL_UNPOOLED": "postgresql://postgres:postgres@localhost:5432/rubic2tripletex"
	}
}
